{
  "name": "smokemon",
  "version": "0.4.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc && pnpm run minify",
    "start-debug": "ts-node ./src/index.ts",
    "start-build": "node ./build/index.js",
    "start-dist": "node ./dist/index.js",
    "force-update": "pnpm update",
    "force-install": "pnpm install",
    "minify": "uglifyjs-folder build -eo dist -x .js",
    "start": "node ./index.js"
  },
  "dependencies": {
    "@discordjs/builders": "^0.8.2",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@top-gg/sdk": "^3.1.3",
    "@types/node": "^16.11.26",
    "datetime-difference": "^1.0.2",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.6.0",
    "dotenv": "^10.0.0",
    "javascript-time-ago": "^2.3.13",
    "knex": "^0.95.15",
    "log4js": "^6.4.4",
    "luxon": "^2.3.1",
    "mnemonist": "^0.38.5",
    "moment": "^2.29.2",
    "mysql2": "^2.3.3",
    "node-fetch": "^2.6.7",
    "pokenode-ts": "^1.13.0",
    "simple-lru-cache": "^0.0.2",
    "topgg-autoposter": "^2.0.1",
    "twitter-lite": "^1.1.0"
  },
  "devDependencies": {
    "@types/javascript-time-ago": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "husky": "7.0.4",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "uglify-js": "^3.15.3",
    "uglifyjs-folder": "^3.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
